{
  "name": "wanderlog",
  "version": "1.0.0",
  "description": "Lust less, remember more. Wanderlog is a travel diary to help you capture your daily highlights as you explore the world.",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean && node tasks/build.js",
    "build": "webpack",
    "prewebpack": "node tasks/build.js",
    "start": "netlify dev",
    "webpack": "webpack-dev-server --hot --config webpack.dev.js",
    "predeploy": "npm run build",
    "deploy": "netlify deploy --prod --dir=dist",
    "gen:schema": "node tasks/exportGraphQLSchema.js",
    "clean": "rimraf dist"
  },
  "author": "Andrew Petersen",
  "license": "ISC",
  "dependencies": {
    "@azure/storage-blob": "^12.3.0",
    "@urql/devtools": "^2.0.2",
