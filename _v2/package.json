    "@urql/introspection": "^0.1.1",
    "chrono-node": "^2.1.10",
    "dayjs": "^1.8.26",
    "framer-motion": "^2.9.5",
    "graphql": "^15.4.0",
    "history": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^3.10.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-select": "^3.1.1",
    "sharp": "^0.26.3",
